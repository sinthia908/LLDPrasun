package com.practice.test;

public abstract class Split {
	private User user;
	private double amount;
	
	Split(User user){
		this.user = user;
	}
	
	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	
	// Abstract method
	public abstract double getAmount();
	
	

}
